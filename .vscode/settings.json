{
    "C_Cpp.errorSquiggles": "disabled",

     "emeraldwalk.runonsave": {
    "commands": [
      {
        "match": ".*",
        "cmd": "powershell -NoProfile -ExecutionPolicy Bypass -File .vscode\\on-save.ps1",
        "runIn": "terminal"
      }
    ]
  },
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 800
}